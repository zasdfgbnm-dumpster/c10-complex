name: CPU-GCC

on:
  push:
    branches: [ master ]
  pull_request:
    branches: [ master ]

jobs:
  build:

    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v2
    - name: build
      run: nvcc -std=c++14 -I. c10/test/util/cuda_complex_test.cu -o test
    - name: run
      run: ./test
    
